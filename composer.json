{
    "name": "tdsereno/httpd-analyzer",
    "description": "Analise de logs apache httpd",
    "type": "library",
    "keywords": [
        "Apache", "httpd", "log", "analytic", "Parse", "Parser"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tdsereno\\HttpdAnalyzer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Tdsereno\\HttpdAnalyzer\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tiago Ereno",
            "email": "tiagoerenoo@gmail.com"
        }
    ],
    "require": {
        "benmorel/apache-log-parser": "^0.2.0",
        "matomo/device-detector": "^6.0",
        "whichbrowser/parser": "^2.1",
        "thadafinser/user-agent-parser": "^2.0",
        "vlucas/phpdotenv": "^5.4",
        "spatie/url": "^1.3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2"
    },   
    "scripts": {
        "phpunit": "./vendor/bin/phpunit tests --debug --colors=always",
        "coverage": "./vendor/bin/phpunit tests --debug --colors=always --coverage-text"
    }
}
